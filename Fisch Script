-- üìç TELEPORT TO COORDINATES

---------------------------------------------------

TPGroup:AddInput("TeleportToCoords", {

    Text = "Teleport to Coordinates",

    Default = "",

    Tooltip = "Enter coordinates like: 100, 50, -200 then press Enter",

    Placeholder = "x, y, z",

    Callback = function(value)

        local x, y, z = value:match("(-?%d+),%s*(-?%d+),%s*(-?%d+)")

        if x and y and z then

            local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

            if root then

                root.CFrame = CFrame.new(tonumber(x), tonumber(y), tonumber(z))

                print(string.format("üß≠ Teleported to: (%d, %d, %d)", x, y, z))

            end

        else

            warn("‚ö†Ô∏è Invalid format. Use: 100, 50, -200")

        end

    end

})

---------------------------------------------------

-- üßç DIRECTIONAL TELEPORT

---------------------------------------------------

local DirectionStuds = 10

local Direction = "Forward"

TPGroup:AddInput("TeleportStudsInput", {

    Text = "Teleport Studs",

    Default = "10",

    Tooltip = "How many studs to teleport",

    Placeholder = "Studs",

    Callback = function(value)

        DirectionStuds = tonumber(value) or 10

    end

})

TPGroup:AddDropdown("TeleportDirection", {

    Values = {"Forward", "Backward", "Left", "Right", "Up", "Down"},

    Default = 1,

    Multi = false,

    Text = "Teleport Direction",

    Tooltip = "Choose a direction to teleport",

    Callback = function(value)

        Direction = value

    end

})

TPGroup:AddButton("TPNOW", {

    Text = "Teleport Now",

    Tooltip = "Teleport in chosen direction by selected studs",

    Func = function()

        local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

        if root then

            local move = Vector3.new(0, 0, 0)

            if Direction == "Forward" then move = root.CFrame.LookVector * DirectionStuds end

            if Direction == "Backward" then move = -root.CFrame.LookVector * DirectionStuds end

            if Direction == "Right" then move = root.CFrame.RightVector * DirectionStuds end

            if Direction == "Left" then move = -root.CFrame.RightVector * DirectionStuds end

            if Direction == "Up" then move = Vector3.new(0, DirectionStuds, 0) end

            if Direction == "Down" then move = Vector3.new(0, -DirectionStuds, 0) end

            root.CFrame = root.CFrame + move

            print("üåÄ Teleported", DirectionStuds, "studs", Direction)

        end

    end

})

QoLGroup:AddToggle("InfiniteOxygenToggle", {

    Text = "Infinite Oxygen / Heat",

    Tooltip = "Disables all scripts inside your Resources folder (Oxygen/Heat)",

    Callback = function(state)

        local folderPath = Workspace:FindFirstChild(LocalPlayer.Name)

        if folderPath then

            local resources = folderPath:FindFirstChild("Resources")

            if resources then

                for _, descendant in ipairs(resources:GetDescendants()) do

                    if descendant:IsA("Script") or descendant:IsA("LocalScript") then

                        descendant.Disabled = state

                    end

                end

                print("‚úÖ Resources scripts have been " .. (state and "disabled" or "enabled") .. ".")

            else

                warn("‚ö†Ô∏è Folder 'Resources' not found inside " .. LocalPlayer.Name)

            end

        else

            warn("‚ö†Ô∏è Player folder not found in workspace.")

        end

    end

})

---------------------------------------------------

-- üåä AUTO OCEAN PLATFORM ABOVE WATER

---------------------------------------------------

local AutoPlatformEnabled = false

local PlatformPart

local PlatformSize = 10

local HeightAboveWater = 3 -- altura acima da √°gua

QoLGroup:AddToggle("AutoOceanPlatformToggle", {

    Text = "Auto Ocean Platform",

    Default = false,

    Tooltip = "Creates a green platform 5 studs above ocean",

    Callback = function(state)

        AutoPlatformEnabled = state

        if not state and PlatformPart then

            PlatformPart:Destroy()

            PlatformPart = nil

        end

        if state then

            task.spawn(function()

                while AutoPlatformEnabled do

                    local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

                    if root then

                        local rayParams = RaycastParams.new()

                        rayParams.FilterDescendantsInstances = {LocalPlayer.Character}

                        rayParams.FilterType = Enum.RaycastFilterType.Exclude

                        -- Raycast para baixo

                        local result = Workspace:Raycast(root.Position, Vector3.new(0, -50, 0), rayParams)

                        if result and result.Instance then

                            local instance = result.Instance

                            local isOcean = false

                            -- Detecta Terrain Water

                            if instance:IsA("Terrain") and instance.Material == Enum.Material.Water then

                                isOcean = true

                            end

                            -- Detecta partes nomeadas 'Ocean'

                            if instance.Name:lower():find("ocean") then

                                isOcean = true

                            end

                            if isOcean then

                                if not PlatformPart then

                                    PlatformPart = Instance.new("Part")

                                    PlatformPart.Anchored = true

                                    PlatformPart.Color = Color3.fromRGB(0, 255, 0)

                                    PlatformPart.Material = Enum.Material.SmoothPlastic

                                    PlatformPart.CanCollide = true

                                    PlatformPart.Name = "OceanPlatform"

                                    PlatformPart.Size = Vector3.new(PlatformSize, 1, PlatformSize)

                                    PlatformPart.Parent = Workspace

                                end

                                -- Posiciona a plataforma 5 studs acima da √°gua

                                PlatformPart.Position = result.Position + Vector3.new(0, HeightAboveWater, 0)

                            elseif PlatformPart then

                                PlatformPart:Destroy()

                                PlatformPart = nil

                            end

                        elseif PlatformPart then

                            PlatformPart:Destroy()

                            PlatformPart = nil

                        end

                    end

                    task.wait(0.05) -- atualiza rapidamente

                end

            end)

        end

    end

})

QoLGroup:AddSlider("PlatformSizeSlider", {

    Text = "Platform Size",

    Default = 10,

    Min = 5,

    Max = 50,

    Rounding = 0,

    Tooltip = "Adjusts the green platform width",

    Callback = function(value)

        PlatformSize = value

        if PlatformPart then

            PlatformPart.Size = Vector3.new(value, 1, value)

        end

    end

})

local AutoReelEnabled = false

local VirtualInputManager = game:GetService("VirtualInputManager")

local Players = game:GetService("Players")

local Camera = workspace.CurrentCamera

local LocalPlayer = Players.LocalPlayer

-- Function to get the rod you are currently holding (Equipped or in Backpack)

local function GetCurrentRod()

    if LocalPlayer.Character then

        for _, tool in pairs(LocalPlayer.Character:GetChildren()) do

            if tool:IsA("Tool") then

                return tool

            end

        end

    end

    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do

        if tool:IsA("Tool") then

            return tool

        end

    end

    return nil

end

-- Function that holds a click at the center of the screen for 0.78 seconds

local function HoldClickCenter()

    local x, y = Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2

    VirtualInputManager:SendMouseButtonEvent(x, y, 0, true, game, 0)

    task.wait(0.78)

    VirtualInputManager:SendMouseButtonEvent(x, y, 0, false, game, 0)

end

-- Add Toggle to Obsidian UI

FishingGroup:AddToggle("AutoReelToggle", {

    Text = "AUTO CAST V2.0",

    Default = false,

    Tooltip = "Automatically clicks the rod when casted is false",

    Callback = function(state)

        AutoReelEnabled = state

        if state then

            task.spawn(function()

                while AutoReelEnabled do

                    local Rod = GetCurrentRod()

                    if Rod and Rod:FindFirstChild("values") then

                        local Values = Rod.values

                        if Values:FindFirstChild("casted") and not Values.casted.Value then

                            HoldClickCenter()

                        end

                    end

                    task.wait(5)

                end

            end)

        end

    end

})

local VirtualInputManager = game:GetService("VirtualInputManager")

local Players = game:GetService("Players")

local Camera = workspace.CurrentCamera

local LocalPlayer = Players.LocalPlayer

local AutoOpenEnabled = false

-- Function to click center

local function ClickCenter()

    local x, y = Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2

    VirtualInputManager:SendMouseButtonEvent(x, y, 0, true, game, 0)

    VirtualInputManager:SendMouseButtonEvent(x, y, 0, false, game, 0)

end

-- Add toggle to Obsidian UI

QoLGroup:AddToggle("AutoOpenToggle", {

    Text = "Auto Open Baits/Crates",

    Default = false,

    Tooltip = "Automatically opens Baits or Crates by clicking the center",

    Callback = function(state)

        AutoOpenEnabled = state

        if state then

            task.spawn(function()

                while AutoOpenEnabled do

                    ClickCenter()

                    task.wait(0.05) -- speed of click

                end

            end)

        end

    end

})

local Players = game:GetService("Players")

local VirtualInputManager = game:GetService("VirtualInputManager")

local player = Players.LocalPlayer

local playerGui = player:WaitForChild("PlayerGui")

-- Offset values (click position adjustment)

local OffsetX = 15  -- right

local OffsetY = 35  -- down

-- Toggle control

local AutoShakeEnabled = false

-- Function that performs the shaking click

local function AutoShake()

	while AutoShakeEnabled do		task.wait(0.1)

		local shakeUI = playerGui:FindFirstChild("shakeui")

		if shakeUI and shakeUI:FindFirstChild("safezone") then

			local button = shakeUI.safezone:FindFirstChild("button")

			if button then

				local title = button:FindFirstChild("title")

				if title and title:IsA("TextLabel") and title.Visible then

					local absPos = title.AbsolutePosition

					local absSize = title.AbsoluteSize

					local clickX = absPos.X + absSize.X / 2 + OffsetX

					local clickY = absPos.Y + absSize.Y / 2 + OffsetY

					pcall(function()

						VirtualInputManager:SendMouseButtonEvent(clickX, clickY, 0, true, game, 0)

						task.wait(0.02)

						VirtualInputManager:SendMouseButtonEvent(clickX, clickY, 0, false, game, 0)

					end)

				end

			end

		end

	end

end

-- Add toggle to the UI

FishingGroup:AddToggle("AutoShakeToggle", {

	Text = "Auto Shake",

	Default = false,

	Tooltip = "Automatically clicks during shake events while fishing.",

	Callback = function(state)

		AutoShakeEnabled = state

		if state then

			task.spawn(AutoShake)

		end

	end

})

