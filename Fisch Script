--=============================

-- FLY

--=============================

local flyEnabled = false

local flySpeed = 50

local flyMethod = "Normal"

local flyConnection

-- Toggle Fly

PlayerMovimentGroup:AddToggle("FlyToggle", {

    Text = "FLY TOGGLE",

    Default = false,

    Tooltip = "Activates/Deactivates fly",

    Callback = function(state)

        flyEnabled = state

        local player = game.Players.LocalPlayer

        local uis = game:GetService("UserInputService")

        if flyConnection then flyConnection:Disconnect() flyConnection = nil end

        if flyEnabled then

            flyConnection = game:GetService("RunService").Heartbeat:Connect(function()

                local char = player.Character

                if char and char:FindFirstChild("HumanoidRootPart") then

                    local root = char.HumanoidRootPart

                    local cam = workspace.CurrentCamera

                    local dir = Vector3.zero

                    if uis:IsKeyDown(Enum.KeyCode.W) then dir += cam.CFrame.LookVector end

                    if uis:IsKeyDown(Enum.KeyCode.S) then dir -= cam.CFrame.LookVector end

                    if uis:IsKeyDown(Enum.KeyCode.A) then dir -= cam.CFrame.RightVector end

                    if uis:IsKeyDown(Enum.KeyCode.D) then dir += cam.CFrame.RightVector end

                    if uis:IsKeyDown(Enum.KeyCode.Space) then dir += cam.CFrame.UpVector end

                    if uis:IsKeyDown(Enum.KeyCode.LeftShift) then dir -= cam.CFrame.UpVector end

                    if flyMethod == "Normal" or flyMethod == "Float Fly" then

                        root.Velocity = dir.Magnitude > 0 and dir.Unit * flySpeed or Vector3.zero

                    elseif flyMethod == "Platform" then

                        local floorY = root.Position.Y - 10

                        root.CFrame = CFrame.new(root.Position.X, floorY + 10, root.Position.Z)

                        root.Velocity = dir.Magnitude > 0 and dir.Unit * flySpeed or Vector3.zero

                    end

                end

            end)

        end

    end

})

-- Fly Speed Slider

PlayerMovimentGroup:AddSlider("FlySpeedSlider", {

    Text = "FLY SPEED",

    Default = flySpeed,

    Min = 10,

    Max = 200,

    Tooltip = "Adjust fly speed",

    Callback = function(value)

        flySpeed = value

    end

})

-- Fly Method Dropdown

PlayerMovimentGroup:AddDropdown("FlyMethodDropdown", {

    Text = "FLY METHOD",

    Values = {"Normal", "Float Fly", "Platform"},

    Default = 1,

    Multi = false,

    Tooltip = "Select fly method",

    Callback = function(value)

        flyMethod = value

    end

})

--=============================

-- PLATFORM

--=============================

local platformEnabled = false

local platformUp = false

local platformSize = Vector2.new(10,10)

local platformUpSpeed = 5

local platformKeybind = Enum.KeyCode.P

local platformPart

-- Platform Toggle

PlayerMovimentGroup:AddToggle("PlatformToggle", {

    Text = "PLATFORM TOGGLE",

    Default = false,

    Tooltip = "Creates a platform under player",

    Callback = function(state)

        platformEnabled = state

        local player = game.Players.LocalPlayer

        local char = player.Character

        if platformEnabled then

            if char and char:FindFirstChild("HumanoidRootPart") then

                if not platformPart then

                    platformPart = Instance.new("Part")

                    platformPart.Size = Vector3.new(platformSize.X,1,platformSize.Y)

                    platformPart.Anchored = true

                    platformPart.CanCollide = true

                    platformPart.Material = Enum.Material.Neon

                    platformPart.Color = Color3.fromRGB(0,255,0)

                    platformPart.CFrame = char.HumanoidRootPart.CFrame - Vector3.new(0,char.HumanoidRootPart.Size.Y/2 + 1,0)

                    platformPart.Parent = workspace

                end

            end

        else

            if platformPart then

                platformPart:Destroy()

                platformPart = nil

            end

        end

    end

})

-- Platform Up Toggle

PlayerMovimentGroup:AddToggle("PlatformUpToggle", {

    Text = "PLATFORM UP TOGGLE",

    Default = false,

    Tooltip = "Enable platform to move up",

    Callback = function(state)

        platformUp = state

    end

})

-- Platform Size Slider

PlayerMovimentGroup:AddSlider("PlatformSizeSlider", {

    Text = "PLATFORM SIZE",

    Default = 10,

    Min = 1,

    Max = 50,

    Tooltip = "Set platform width/length",

    Callback = function(value)

        platformSize = Vector2.new(value,value)

        if platformPart then

            platformPart.Size = Vector3.new(platformSize.X,1,platformSize.Y)

        end

    end

})

-- Platform Up Speed Slider

PlayerMovimentGroup:AddSlider("PlatformUpSpeedSlider", {

    Text = "PLATFORM UP SPEED",

    Default = platformUpSpeed,

    Min = 1,

    Max = 50,

    Tooltip = "Set platform rising speed",

    Callback = function(value)

        platformUpSpeed = value

    end

})

-- Platform Loop

spawn(function()

    while task.wait(0.1) do

        if platformEnabled and platformPart then

            local player = game.Players.LocalPlayer

            local char = player.Character

            if char and char:FindFirstChild("HumanoidRootPart") then

                local root = char.HumanoidRootPart

                platformPart.CFrame = CFrame.new(root.Position.X, root.Position.Y - 3, root.Position.Z)

                if platformUp then

                    platformPart.CFrame += Vector3.new(0,platformUpSpeed * 0.1,0)

                end

            end

        end

    end

end)

-- Services

local ReplicatedStorage = game:GetService("ReplicatedStorage")

local Players = game:GetService("Players")

local LocalPlayer = Players.LocalPlayer

local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- RemoteEvents

local reelfinished = ReplicatedStorage:WaitForChild("events"):WaitForChild("reelfinished")

-- Args

local reelArgs = {100, true}

local castArgs = {100, 1}

-- Vari√°veis de controle

local AutoReelEnabled = false

local AutoCastEnabled = false

local AutoReelLoop

local AutoCastLoop

-- REEL FISH (uma vez)

FishingGroup:AddButton("REELFISH", {

    Text = "REEL FISH",

    Tooltip = "Fire the Reel Remote once",

    Func = function()

        pcall(function()

            reelfinished:FireServer(unpack(reelArgs))

        end)

    end

})

-- AUTO REEL (loop)

FishingGroup:AddToggle("AutoReelToggle", {

    Text = "AUTO REEL",

    Default = false,

    Tooltip = "Automatically fire the Reel Remote",

    Callback = function(state)

        AutoReelEnabled = state

        if AutoReelLoop then

            task.cancel(AutoReelLoop)

            AutoReelLoop = nil

        end

        if AutoReelEnabled then

            AutoReelLoop = task.spawn(function()

                while AutoReelEnabled do

                    pcall(function()

                        reelfinished:FireServer(unpack(reelArgs))

                    end)

                    task.wait(1)

                end

            end)

        end

    end

})

-- CAST ROD (uma vez)

FishingGroup:AddButton("CASTROD", {

    Text = "CAST ROD",

    Tooltip = "Fire castAsync on your Rod once",

    Func = function()

        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

        local rod = nil

        for _, tool in pairs(char:GetChildren()) do

            if tool:IsA("Tool") and string.find(tool.Name:lower(), "rod") then

                rod = tool

                break

            end

        end

        if rod and rod:FindFirstChild("events") and rod.events:FindFirstChild("castAsync") then

            pcall(function()

                rod.events.castAsync:InvokeServer(unpack(castArgs))

            end)

        end

    end

})

-- AUTO CAST ROD (loop)

FishingGroup:AddToggle("AutoCastToggle", {

    Text = "AUTO CAST ROD",

    Default = false,

    Tooltip = "Automatically cast your Rod",

    Callback = function(state)

        AutoCastEnabled = state

        if AutoCastLoop then

            task.cancel(AutoCastLoop)

            AutoCastLoop = nil

        end

        if AutoCastEnabled then

            AutoCastLoop = task.spawn(function()

                while AutoCastEnabled do

                    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

                    local rod = nil

                    for _, tool in pairs(char:GetChildren()) do

                        if tool:IsA("Tool") and string.find(tool.Name:lower(), "rod") then

                            rod = tool

                            break

                        end

                    end

                    if rod and rod:FindFirstChild("events") and rod.events:FindFirstChild("castAsync") then

                        pcall(function()

                            rod.events.castAsync:InvokeServer(unpack(castArgs))

                        end)

                    end

                    task.wait(0.1)

                end

            end)

        end

    end

})

