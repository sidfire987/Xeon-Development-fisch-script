TPGroup:AddButton("COPYCORDINATES", {

    Text = "Copy Coordinates",

    Tooltip = "Copies your current XYZ position",

    Func = function()

        local root = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

        if root then

            setclipboard(string.format("%d, %d, %d", root.Position.X, root.Position.Y, root.Position.Z))

            print("üìã Coordinates copied:", root.Position)

        end

    end

})

