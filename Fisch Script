local repo = "https://raw.githubusercontent.com/deividcomsono/Obsidian/main/"

local Library = loadstring(game:HttpGet(repo .. "Library.lua"))()

local ThemeManager = loadstring(game:HttpGet(repo .. "addons/ThemeManager.lua"))()

local SaveManager = loadstring(game:HttpGet(repo .. "addons/SaveManager.lua"))()

-- Cria a janela principal

local Window = Library:CreateWindow({

    Title = "Xeon DEVELOPMENT",

    Footer = "v2.59.04",

    Icon = 95816097006870,

    NotifySide = "Right",

    ShowCustomCursor = true,

    Theme = "Pink",

})

-- Aba √∫nica Main

local MainTab = Window:AddTab("Main", "house")

-- Cria√ß√£o de GroupBoxes no estilo do script Blox Fruits

local GroupBoxes = {}

local groupboxNames = { "Fishing Features", "Player Moviment", "TP Features", "Quality Of Life" }

for i, name in ipairs(groupboxNames) do

    if i % 2 == 1 then

        GroupBoxes[name] = MainTab:AddLeftGroupbox(name)

    else

        GroupBoxes[name] = MainTab:AddRightGroupbox(name)

    end

end

local FishingGroup = GroupBoxes["Fishing Features"]

local PlayerMovimentGroup = GroupBoxes["Player Moviment"]

local TPGroup = GroupBoxes["TP Features"]

local QoLGroup = GroupBoxes["Quality Of Life"]

--=============================

-- SPEED

--=============================

local speedEnabled = false

local speedPower = 75

local speedMethod = "Humanoid"

-- Toggle Speed9

PlayerMovimentGroup:AddToggle("SpeedToggle", {

    Text = "SPEED TOGGLE",

    Default = false,

    Tooltip = "Activates/Deactivates speed loop",

    Callback = function(state)

        speedEnabled = state

    end

})

-- Speed Power Slider

PlayerMovimentGroup:AddSlider("SpeedPowerSlider", {

    Text = "SPEED POWER",

    Default = speedPower,

    Min = 16,

    Max = 500,

    Tooltip = "Set speed power",

    Callback = function(value)

        speedPower = value

    end

})

-- Speed Method Dropdown

PlayerMovimentGroup:AddDropdown("SpeedMethodDropdown", {

    Text = "SPEED METHOD",

    Values = {"Humanoid", "CFrame Teleport", "Velocity"},

    Default = 1,

    Multi = false,

    Tooltip = "Choose speed method",

    Callback = function(value)

        speedMethod = value

    end

})

-- Loop que aplica o Speed

spawn(function()

    while task.wait(0.1) do

        if speedEnabled then

            local char = game.Players.LocalPlayer.Character

            if char and char:FindFirstChildOfClass("Humanoid") then

                local humanoid = char:FindFirstChildOfClass("Humanoid")

                if speedMethod == "Humanoid" then

                    humanoid.WalkSpeed = speedPower

                elseif speedMethod == "CFrame Teleport" then

                    local root = char:FindFirstChild("HumanoidRootPart")

                    if root then

                        root.CFrame += root.CFrame.LookVector * (speedPower * 0.1)

                    end

                elseif speedMethod == "Velocity" then

                    local root = char:FindFirstChild("HumanoidRootPart")

                    if root then

                        root.Velocity = root.CFrame.LookVector * speedPower

                    end

                end

            end

        end

    end

end)

--=============================

-- JUMP POWER

--=============================

local jumpEnabled = false

local jumpPower = 100

local jumpCount = 1

local jumpMethod = "Humanoid"

-- Toggle JumpPower

PlayerMovimentGroup:AddToggle("JumpPowerToggle", {

    Text = "JUMP POWER TOGGLE",

    Default = false,

    Tooltip = "Activates/Deactivates jump power loop",

    Callback = function(state)

        jumpEnabled = state

    end

})

-- Jump Power Slider

PlayerMovimentGroup:AddSlider("JumpPowerSlider", {

    Text = "JUMP POWER",

    Default = jumpPower,

    Min = 50,

    Max = 500,

    Tooltip = "Set jump power",

    Callback = function(value)

        jumpPower = value

    end

})

-- Jump Count Input

PlayerMovimentGroup:AddInput("JumpCountInput", {

    Text = "JUMP COUNT",

    Default = tostring(jumpCount),

    Numeric = true,

    Finished = true,

    Tooltip = "Set number of jumps",

    Callback = function(value)

        jumpCount = tonumber(value) or 1

    end

})

-- Jump Method Dropdown

PlayerMovimentGroup:AddDropdown("JumpPowerMethodDropdown", {

    Text = "JUMP POWER METHOD",

    Values = {"Humanoid", "CFrame", "Velocity"},

    Default = 1,

    Multi = false,

    Tooltip = "Choose jump method",

    Callback = function(value)

        jumpMethod = value

    end

})

-- Loop que aplica JumpPower

spawn(function()

    while task.wait(0.1) do

        if jumpEnabled then

            local char = game.Players.LocalPlayer.Character

            if char and char:FindFirstChildOfClass("Humanoid") then

                local humanoid = char:FindFirstChildOfClass("Humanoid")

                for i = 1, jumpCount do

                    if jumpMethod == "Humanoid" then

                        humanoid.JumpPower = jumpPower

                        humanoid:ChangeState("Jumping")

                    elseif jumpMethod == "CFrame" then

                        local root = char:FindFirstChild("HumanoidRootPart")

                        if root then

                            root.CFrame += Vector3.new(0, jumpPower * 0.1, 0)

                        end

                    elseif jumpMethod == "Velocity" then

                        local root = char:FindFirstChild("HumanoidRootPart")

                        if root then

                            root.Velocity = Vector3.new(0, jumpPower, 0)

                        end

                    end

                    task.wait(0.05)

                end

            end

        end

    end

end)

