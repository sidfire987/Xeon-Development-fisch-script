local noclipEnabled = false

local noclipMethod = "Normal"

-- Toggle Noclip

PlayerMovimentGroup:AddToggle("NoclipToggle", {

    Text = "NOCLIP TOGGLE",

    Default = false,

    Tooltip = "Activates/Deactivates noclip",

    Callback = function(state)

        noclipEnabled = state

    end

})

-- Noclip Method Dropdown

PlayerMovimentGroup:AddDropdown("NoclipMethodDropdown", {

    Text = "NOCLIP METHOD",

    Values = {"Normal", "Noclip TP"},

    Default = 1,

    Multi = false,

    Tooltip = "Choose noclip method",

    Callback = function(value)

        noclipMethod = value

    end

})

-- Loop Noclip

spawn(function()

    while task.wait(0.1) do

        if noclipEnabled then

            local char = game.Players.LocalPlayer.Character

            if char then

                if noclipMethod == "Normal" then

                    for _, part in pairs(char:GetDescendants()) do

                        if part:IsA("BasePart") then

                            part.CanCollide = false

                        end

                    end

                elseif noclipMethod == "Noclip TP" then

                    local root = char:FindFirstChild("HumanoidRootPart")

                    if root then

                        -- Teleporta a cada 0.1s para a pr√≥pria posi√ß√£o para evitar colis√£o

                        root.CFrame = root.CFrame

                    end

                end

            end

        end

    end

end)

