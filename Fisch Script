-- üß≠ Alphabetical Sort

local Values = {}

for name, _ in pairs(TeleportLocations) do

    table.insert(Values, name)

end

table.sort(Values, function(a, b)

    if a == "None" then return true end

    if b == "None" then return false end

    return a < b

end)

-- üåÄ Dropdown (Obsidian UI)

TPGroup:AddDropdown("TPNISLANDS", {

    Title = "TELEPORT LOCATION",

    Desc = "SELECT A PLACE TO TELEPORT",

    Values = Values,

    Value = "None",

    Callback = function(selected)

        local player = game.Players.LocalPlayer

        local char = player.Character or player.CharacterAdded:Wait()

        local hrp = char:WaitForChild("HumanoidRootPart")

        if TeleportLocations[selected] then

            hrp.CFrame = CFrame.new(TeleportLocations[selected])

        end

    end

})

local AutoScyllaEnabled = false

PZGroup:AddToggle("AutoScylla", {

    Text = "Auto Scylla Puzzle",

    Default = false,

    Tooltip = "Automatically completes the Scylla Puzzle",

    Callback = function(state)

        AutoScyllaEnabled = state

        if AutoScyllaEnabled then

            task.spawn(function()

                local Players = game:GetService("Players")

                local LocalPlayer = Players.LocalPlayer

                local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

                local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

                local ReplicatedStorage = game:GetService("ReplicatedStorage")

                local function TeleportAndWait(pos, waitTime)

                    HumanoidRootPart.CFrame = CFrame.new(pos)

                    task.wait(waitTime)

                end

                local function InteractPrompts()

                    -- Loop through all ProximityPrompts and fire them instantly

                    for _, prompt in pairs(workspace:GetDescendants()) do

                        if prompt:IsA("ProximityPrompt") and prompt.Enabled then

                            prompt:InputHoldBegin()

                            task.wait(0.1)

                            prompt:InputHoldEnd()

                        end

                    end

                end

                -- Step 1: TP to Veil of Forksaken Door 1

                TeleportAndWait(Vector3.new(-4354.93, -11186.15, 7407.52), 70)

                -- Step 2: TP to -4795, -11187, 6597

                TeleportAndWait(Vector3.new(-4795, -11187, 6597), 5)

                InteractPrompts()

                -- Step 3: TP to -4347, -11152, 6485

                TeleportAndWait(Vector3.new(-4347, -11152, 6485), 5)

                InteractPrompts()

                -- Step 4: TP to -4034, -11192, 6493

                TeleportAndWait(Vector3.new(-4034, -11192, 6493), 5)

                InteractPrompts()

                -- Step 5: TP to -4357, -11092, 7138

                TeleportAndWait(Vector3.new(-4357, -11092, 7138), 1)

                

                -- Equip Crossbow bow and interact

                local function EquipAndInteract(itemName)

                    local tool = LocalPlayer.Backpack:FindFirstChild(itemName) or Character:FindFirstChild(itemName)

                    if tool then

                        tool.Parent = Character

                    end

                    task.wait(0.5)

                    InteractPrompts()

                end

                EquipAndInteract("Crossbow bow")

                EquipAndInteract("Crossbow base")

                EquipAndInteract("Crossbow arrow")

                -- Finished

                warn("‚úÖ Auto Scylla Puzzle finished!")

            end)

        end

    end

})
